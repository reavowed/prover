{
    "compilerOptions": {
        "allowJs": false,
        "target": "es6",
        "jsx": "preserve",
        "moduleResolution": "node",
        "noImplicitThis": true,
        "noImplicitReturns": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "strictPropertyInitialization": true,
      "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/main/js/**/*"
    ]
}
